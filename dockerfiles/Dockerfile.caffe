# Generated by create_caffe_dockerfile.py
FROM ubuntu:16.04
RUN apt-get update && apt-get install -y git
RUN git clone --recursive https://github.com/BVLC/caffe

COPY Makefile.config.caffe /caffe/Makefile.config
RUN cd /caffe
RUN make all -j9
RUN make pycaffe
RUN mkdir /shared
VOLUME /shared
RUN echo 'export PYTHONPATH="/caffe/python"' >> /root/.profile
    